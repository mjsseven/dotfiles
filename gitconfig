[user]
  name = Salvatore Realmuto
  email = srealmuto@ise.com
[color]
  ui = true
[color "branch"]
  current = blue bold
  local = yellow
  remote = magenta
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  whitespace = white reverse
[color "status"]
  added = green bold
  changed = blue bold
  untracked = magenta bold
  deleted = red bold
  branch = yellow bold
[alias]
  authors = shortlog -sn
  behind = !git log ..upstream/master --oneline
  ahead = !git log upstream/master.. --oneline
  upstream = !git cherry upstream/master
  files = !git diff --name-only upstream/master..
  dupes = !git rev-list --objects --all | cut -c1-7 | sort | uniq -c | grep -v '^ *1'
  count = !git shortlog -n --no-merges --since '{last friday at 09:30:00}'
  undo = reset HEAD@{1}
	et = status
  di = diff
  ci = commit
  br = branch
  llog = log --date=local
[rerere]
  enabled = true
[push]
  default = simple
